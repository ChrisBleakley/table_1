package GUI;

public class Army {
	
	public Army(Integer size, Integer playerid, Integer countryindex){
		this.size = size;
		this.playerid = playerid;
		this.countryid = countries;
	}
	public void setSize(Integer size){
		this.size = size;
	}
	public Integer getSize(){
